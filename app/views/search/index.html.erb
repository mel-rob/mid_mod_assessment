<h1><%= search_results.get_members.count %> for <%= params[:house] %></h1>
<% search_results.get_members.each do |member| %>
  <ul class='house-members'>
      <li class='name'>Name: <%= member.name %></li>
      <% if member.role %>
        <li class='role'>Role: <%= member.role %></li>
      <% end %>
      <li class='house'>House: <%= member.house %></li>
      <% if member.patronus %>
        <li class='patronus'>Patronus: <%= member.patronus %></li></ul>
      <% end %>
    </ul>
<% end %>
